{
  "manifest_version": 3,
  "name": "Unified Problem Finder",
  "version": "1.0",
  "description": "Search for coding problems across LeetCode, LintCode, and other curated lists.",
  "action": {
    "default_title": "Unified Problem Finder",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}

